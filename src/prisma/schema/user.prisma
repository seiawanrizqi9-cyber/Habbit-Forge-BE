model User {
  id         String       @id @default(uuid())
  email      String       @unique
  createdAt  DateTime     @default(now())
  updatedAt  DateTime     @updatedAt
  username   String       @unique
  password   String
  checkIn    CheckIn[]
  dailyStats DailyStats[]
  habit      Habit[]
  categories Category[]
  profile    Profile?

  @@map("users")
}
